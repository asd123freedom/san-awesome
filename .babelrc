{
  "presets": [
    ["env", { "modules": false}],
    "stage-2"
  ],
  "plugins": [
    "transform-es2015-modules-commonjs",
    [
      "transform-runtime",
      {
        "polyfill": false
      }
    ]
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [ "istanbul" ]
    }
  }
}
